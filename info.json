{
  "id": "plugin.com.gaivrt.gemini", 
  "plugin_type": "translate",
  "icon": "gemini.svg", 
  "display": "Google Gemini", 
  "homepage": "https://github.com/gaivrt/pot-app-translate-plugin-gemini", 
  "needs": [ 
    {
      "key": "apiKey",
      "display": "Gemini API Key",
      "type": "input"
    },
    {
    "key": "modelName",
    "display": "Model Name",
    "type": "input",
    "default": "gemini-2.0-flash"
    },
    { 
      "key": "enableStreaming",
      "display": "启用流式输出",
      "type": "select", 
      "options": { 
        "false": "否 (No)", 
        "true": "是 (Yes)"  
      },
      "default": "false" 
    }
  ],
  
  "language": {
    "auto": "auto",
    "zh_cn": "zh",
    "zh_tw": "zh_HANT",
    "en": "en",
    "ja": "ja",
    "ko": "ko",
    "fr": "fr",
    "es": "es",
    "ru": "ru",
    "de": "de",
    "it": "it",
    "tr": "tr",
    "pt_pt": "pt",
    "pt_br": "pt",
    "vi": "vi",
    "id": "id",
    "th": "th",
    "ms": "ms",
    "ar": "ar",
    "hi": "hi",
    "mn_cy": "mn",
    "mn_mo": "mn",
    "km": "km",
    "nb_no": "no",
    "nn_no": "no",
    "fa": "fa"
  }
}
