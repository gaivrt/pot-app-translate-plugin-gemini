{
  "id": "plugin.io.github.gaivrt.gemini", 
  "plugin_type": "translate",
  "icon": "gemini.svg", 
  "display": "Google Gemini", 
  "homepage": "https://github.com/gaivrt/pot-app-translate-plugin-gemini", 
  "needs": [ 
    {
      "key": "apiKey",
      "display": "Gemini API Key",
      "type": "input"
    },
    {
    "key": "modelName",
    "display": "Model Name",
    "type": "input",
    "default": "gemini-2.0-flash"
    },
    {
      "key": "systemPrompt",
      "display": "自定义系统指令",
      "type": "input",
      "default": "You are a faithful, expressive, and elegant translator. Output ONLY the translated text, without any introductory phrases, explanations, or markdown formatting like quotes or code blocks.\n"
    }
    ],
  
  "language": {
    "auto": "auto",
    "zh_cn": "zh",
    "zh_tw": "zh_HANT",
    "en": "en",
    "ja": "ja",
    "ko": "ko",
    "fr": "fr",
    "es": "es",
    "ru": "ru",
    "de": "de",
    "it": "it",
    "tr": "tr",
    "pt_pt": "pt",
    "pt_br": "pt",
    "vi": "vi",
    "id": "id",
    "th": "th",
    "ms": "ms",
    "ar": "ar",
    "hi": "hi",
    "mn_cy": "mn",
    "mn_mo": "mn",
    "km": "km",
    "nb_no": "no",
    "nn_no": "no",
    "fa": "fa"
  }
}
